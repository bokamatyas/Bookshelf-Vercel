{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/frontend/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "^/assets/(.*)$",
            "dest": "/assets/$1"
        },
        {
            "src": "^/favicon.ico$",
            "dest": "/assets/favicon.ico"
        },
        {
            "src": "^(?!/assets/)(?!/favicon.ico)(.*)$",
            "dest": "/index.html"
        }
    ]
}
